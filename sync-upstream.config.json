{
  "upstreamRepo": "https://github.com/vbenjs/vue-vben-admin.git",
  "upstreamBranch": "main",
  "companyBranch": "master",
  "commitMessage": " 合并上游仓库内容到指定目录",
  "autoPush": false,
  "forceOverwrite": true,
  "verbose": false,
  "silent": false,
  "dryRun": false,
  "previewOnly": false,
  "syncDirs": ["packages", "scripts", "internal"],
  "concurrencyLimit": 10,
  "retryConfig": {
    "maxRetries": 3,
    "initialDelay": 2000,
    "backoffFactor": 1.5
  },
  "conflictResolutionConfig": {
    "defaultStrategy": "auto-merge",
    "autoResolveTypes": [".js", ".ts", ".jsx", ".tsx", ".css", ".scss", ".html", ".md"],
    "logResolutions": true
  },
  "branchStrategyConfig": {
    "enable": false,
    "strategy": "feature",
    "baseBranch": "main",
    "branchPattern": "feature/{name}",
    "autoSwitchBack": true,
    "autoDeleteMergedBranches": false
  },
  "cacheConfig": {
    "expiryMs": 604800000,
    "maxSizeBytes": 5368709120,
    "lruEnabled": true,
    "lruMaxEntries": 1000
  }
}
